<?xml version="1.0" encoding="utf-8"?>

<RunSettings>
    <RunConfiguration>
        <EnvironmentVariables>
            <PWDEBUG>0</PWDEBUG>
        </EnvironmentVariables>
    </RunConfiguration>
    <!-- Playwright -->
    <Playwright>
        <BrowserName>chromium</BrowserName>
        <ExpectTimeout>30000</ExpectTimeout>
        <LaunchOptions>
            <Headless>false</Headless>
            <SlowMo>1000</SlowMo>
            <DevTools>true</DevTools>
        </LaunchOptions>
    </Playwright>
</RunSettings> 